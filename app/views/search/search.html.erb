<table style="width:90%">
  <tr>
    <td>Events</td>

  </tr>
  <tr>
    <td>
      <% @events.each do |event| %>
          <%= link_to event_path(event), class: "columnheadermess"  do %>
              <div class="blogpostentrys">
                <h3><%= highlight(event.name, @word) %></h3><br>
                <% allstrings = stringswithwords(@word, event) %>
                <% allstrings.each do |string| %>
                    <%= highlight(string.to_s, @word) %> <br><br>
                 <% end %>
                <br>
                <br>
                <% if !(wordmissing?(event, @word).blank?) %>
                    <br>
                    Es fehlt:
                <% wordmissing?(event, @word).each do |word| %>
                    <%= word %>
                <% end %>
                <% end %>
              </div>
          <% end %>
      <% end %>
    </td>
    <td>

    </td>
    <td>

    </td>
  </tr>
</table>

<script src="assets/creative.js"></script>